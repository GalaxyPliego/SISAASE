<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <constant name="struts.devMode" value="true" />

    <package name="basicstruts2" extends="struts-default, json-default">
        <action name="index">
            <result>/index.jsp</result>
        </action>

        <action name="horario">
            <result>/jsp/permanencia/grupos/gestionHorarios/listadoHorario.jsp</result>
        </action>

        <action name="coordinador">
            <result>/jsp/permanencia/perfil/coordinador/homeCoordinador.jsp</result>
        </action>

        <action name="consultarHorarioAsesoria" method="consultarHorarioAsesoria" class="mx.edu.utez.sisaase.permanencia.action.ActionHorarioAsesoria">
            <result type="json"/>
        </action>


    </package>

</struts>